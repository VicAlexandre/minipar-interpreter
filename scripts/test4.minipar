func contagem_regressiva(inicio: number) -> number {
  contador: number = inicio
  final: number = 0

  while (contador > 0) {
    contador = contador - 1
  }

  while (true) {
      final = 100
      break
  }

  dummy: number = 0
  while (dummy < 2) {
      dummy = dummy + 1
      if (dummy == 1) {
          continue 
      }
      final = final + 1
  }

  return final
}